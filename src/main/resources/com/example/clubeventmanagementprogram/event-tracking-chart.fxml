<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.clubeventmanagementprogram.controller.EventTrackingChartController">
   <children>
      <Button fx:id="backButton" layoutX="100.0" layoutY="10.0" text="Go back" mnemonicParsing="false" onAction="#handleGoBack" />
      <Label fx:id="trackEventsLabel" layoutX="275.0" layoutY="10.0" text="Track Events" />
      <TableView fx:id="eventTable" editable="true" layoutX="14.0" layoutY="40.0" prefHeight="346.0" prefWidth="572.0">
         <columns>
            <TableColumn fx:id="eventNameColumn" text="Event Name" />
            <TableColumn fx:id="eventDescriptionColumn" text="Event Description" />
            <TableColumn fx:id="dateColumn" text="Date" />
            <TableColumn fx:id="startTimeColumn" text="Start Time" />
            <TableColumn fx:id="endTimeColumn" text="End Time" />
         </columns>
      </TableView>
   </children>
</AnchorPane>