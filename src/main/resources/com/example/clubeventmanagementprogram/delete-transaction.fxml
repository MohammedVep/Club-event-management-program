<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.clubeventmanagementprogram.controller.FinancialTransactionActions.DeleteTransactionController">
    <Label fx:id="confirmDeleteLabel" text="Are you sure you want to delete this transaction?" />
    <HBox alignment="CENTER" spacing="10.0">
        <Button fx:id="deleteButton" onAction="#deleteSelectedFinancialTransaction" text="Yes"/>
        <Button fx:id="cancelButton" onAction="#loadFinancialTransactionScene" text="No"/>
    </HBox>
</VBox>