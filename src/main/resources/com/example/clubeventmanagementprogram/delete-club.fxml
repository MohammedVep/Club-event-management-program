<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" >
    <Label fx:id="confirmDeleteLabel" text="Are you sure you want to delete this club?" />
    <HBox alignment="CENTER" spacing="10.0">
        <Button fx:id="deleteButton" onAction="#deleteSelectedClub" text="Yes"/>
        <Button fx:id="cancelButton" onAction="#loadClubScene" text="No"/>
    </HBox>
</VBox>